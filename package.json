{
  "name": "eventrix",
  "version": "1.1.3",
  "description": "store based on events",
  "main": "index.js",
  "repository": "https://github.com/mprzodala/eventrix",
  "homepage": "https://github.com/mprzodala/eventrix",
  "scripts": {
    "start": "webpack-dev-server --hot --progress --colors",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack -p --config webpack-prod.config.js",
    "build:demo": "webpack -p --config webpack.config.js",
    "coverage": "jest --coverage",
    "coverage:report": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint -c .eslintrc.json **/*.js? --ignore-path .gitignore",
    "lint:fix": "eslint --fix -c .eslintrc.json **/*.js? --ignore-path .gitignore"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "store"
  ],
  "author": "Mariusz Przoda≈Ça",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.3",
    "@babel/traverse": "^7.7.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.1.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "7.13.0",
    "jest": "^24.9.0",
    "pre-commit": "^1.2.2",
    "react-proptypes": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "jest": {
    "setupFiles": [
      "./enzymeConfig.js"
    ],
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "collectCoverage": true
  }
}
